{
  "name": "@coderich/autograph",
  "author": "Richard Livolsi (coderich)",
  "version": "0.7.42",
  "description": "AutoGraph",
  "keywords": [
    "graphql",
    "mongo",
    "neo4j",
    "redis",
    "dataloader",
    "resolver",
    "orm"
  ],
  "main": "index.js",
  "license": "MIT",
  "files": [
    "src/",
    "index.js"
  ],
  "scripts": {
    "start": "APP_ROOT_PATH=$(pwd) node ./test/server",
    "test": "APP_ROOT_PATH=$(pwd) ratchet test",
    "test:debug": "APP_ROOT_PATH=$(pwd) node --inspect-brk ./node_modules/jest/bin/jest.js --watch --runInBand",
    "lint": "APP_ROOT_PATH=$(pwd) ratchet lint",
    "inspect": "APP_ROOT_PATH=$(pwd) node --expose-gc --inspect=9222 ./src/server",
    "ratchet": "ratchet"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.0",
    "axios": "^0.21.1",
    "dataloader": "^2.0.0",
    "deepmerge": "^4.2.2",
    "fill-range": "^7.0.1",
    "glob": "^7.1.6",
    "graphql-fields": "^2.0.3",
    "graphql-tools": "^5.0.0",
    "lodash": "^4.17.20",
    "object-hash": "^2.0.1",
    "picomatch": "^2.1.1",
    "uuid": "^3.3.3",
    "validator": "^12.2.0"
  },
  "devDependencies": {
    "@coderich/ratchet": "^1.5.4",
    "apollo-server": "^2.16.1",
    "graphql": "^15.5.0",
    "graphql-import": "^1.0.2",
    "mongodb": "3.6.3",
    "mongodb-memory-server": "^6.2.2",
    "neo4j-driver": "^4.0.0",
    "neodb": "^3.0.0",
    "redis": "^2.8.0",
    "redis-mock": "^0.47.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:coderich/autograph.git"
  }
}
